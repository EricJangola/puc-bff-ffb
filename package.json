{
  "name": "espacio-api",
  "version": "0.1.0",
  "license": "ISC",
  "private": true,
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "lint:typescript": "tsc --noEmit",
    "lint:eslint": "eslint --cache --fix --ext '.js,.ts,.tsx' .",
    "lint": "run-p lint:typescript lint:eslint",
    "start": "nodemon --ext 'js,ts' --exec babel-node --extensions '.js,.ts,.tsx' src/index.ts --insecure-http-parser",
    "test": "jest --detectOpenHandles --forceExit",
    "build:clean": "rimraf dist",
    "build:app": "babel src --out-dir dist/app --extensions '.js,.ts,.tsx' --copy-files --no-copy-ignored --source-maps",
    "build:debug": "npm-run-all -s build:clean \"build:app --source-maps\"",
    "build": "npm-run-all -s build:clean -p build:app",
    "migrate:make": "babel-node ./node_modules/.bin/knex migrate:make --knexfile ./database/knexfile.ts",
    "migrate:latest": "babel-node ./node_modules/.bin/knex migrate:latest --knexfile ./database/knexfile.ts",
    "migrate:rollback": "babel-node ./node_modules/.bin/knex migrate:rollback --knexfile ./database/knexfile.ts",
    "prepare": "./node_modules/.bin/husky install"
  },
  "dependencies": {
    "@types/lodash": "^4.14.182",
    "@babel/cli": "7.15.7",
    "@babel/core": "7.15.5",
    "@babel/node": "7.15.4",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/preset-env": "7.15.6",
    "@babel/preset-typescript": "7.15.0",
    "babel-plugin-module-resolver": "4.1.0",
    "amqplib": "0.10.0",
    "axios": "0.27.2",
    "bcryptjs": "2.4.3",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "cypress": "^9.6.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "date-fns": "2.28.0",
    "nodemon": "2.0.12",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "firebase": "^9.9.2",
    "firebase-admin": "^11.0.1",
    "form-data": "^4.0.0",
    "helmet": "4.6.0",
    "joi": "17.5.0",
    "jsonwebtoken": "^8.5.1",
    "lodash.get": "^4.4.2",
    "lodash.omit": "4.5.0",
    "lodash.pick": "4.4.0",
    "lodash.set": "^4.3.2",
    "morgan": "1.10.0",
    "multer": "1.4.4",
    "nodemailer": "6.7.2",
    "pg": "^8.7.3",
    "uuid": "8.3.2",
    "winston": "3.7.2",
    "xml2js": "^0.4.23"
  },
  "cypress-cucumber-preprocessor": {
    "step_definitions": "cypress/support/steps"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/jackrabbit": "^4.3.3",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "8.5.6",
    "@types/lodash.get": "^4.4.7",
    "@types/lodash.omit": "4.5.6",
    "@types/lodash.pick": "4.4.6",
    "@types/lodash.set": "^4.3.7",
    "@types/morgan": "1.9.3",
    "@types/multer": "1.4.7",
    "@types/newrelic": "7.0.3",
    "@types/node": "14.14.31",
    "@types/nodemailer": "6.4.4",
    "@types/sharp": "0.29.5",
    "@types/uuid": "8.3.3",
    "@types/xml2js": "^0.4.11",
    "@typescript-eslint/eslint-plugin": "5.21.0",
    "@typescript-eslint/parser": "5.21.0",
    "eslint": "8.14.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-import-helpers": "1.2.1",
    "eslint-plugin-jest": "24.4.2",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "7.0.4",
    "jest": "^27.5.1",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "typescript": "4.6.3"
  },
  "packageManager": "yarn@3.1.1"
}
